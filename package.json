{
  "name": "@sdmx/faceted-search-ui",
  "version": "0.1.0",
  "author": "redpelicans",
  "description": "Faceted Search UI for Enterprise SDMX Search Engine",
  "keywords": [
    "SDMX",
    "Elastic Search",
    "NodeJS",
    "Faceted Search"
  ],
  "tags": [
    "SDMX",
    "NodeJS",
    "Faceted Search"
  ],
  "dependencies": {
    "@blueprintjs/core": "^1.23.1",
    "@types/es6-shim": "^0.31.34",
    "@types/moment": "^2.13.0",
    "@types/pure-render-decorator": "^0.2.27",
    "@types/react": "^15.0.39",
    "@types/react-addons-css-transition-group": "^15.0.3",
    "@types/react-dom": "^15.5.1",
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "normalize.css": "^7.0.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.0",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^15.6.1",
    "react-error-overlay": "^1.0.9",
    "react-media": "^1.6.1",
    "react-motion": "^0.5.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "NODE_ENV=test mocha --require ignore-styles --require babel-core/register --reporter spec \"./**/__test__/*.js\" ",
    "lint": "eslint src --color --fix",
    "test:coverage": "NODE_ENV=test nyc yarn test",
    "build:stats": "NODE_ENV=production webpack --config config/webpack.config.prod.js --profile --json > ./stats.json",
    "build:analyse": "webpack-bundle-analyzer ./stats.json ./build --report --mode=static"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "ignore-styles": "^5.0.1",
    "mocha": "^3.4.2",
    "nyc": "^11.1.0",
    "react-test-renderer": "^15.6.1",
    "should": "^11.2.1",
    "webpack-bundle-analyzer": "^2.8.3"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "nyc": {
    "exclude": [
      "build"
    ]
  }
}
